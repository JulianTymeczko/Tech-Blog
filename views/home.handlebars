<main>
  {{#each data}}
    <div class="card">

      <div class="card-header">
        Posted by
        {{this.user}}
        on
        {{this.date}}
      </div>
      <div class="card-body">
        <h5 class="card-title">Title: {{this.title}}</h5>
        <p class="card-text">{{this.paragraph}}</p>
      </div>

    </div>
  {{/each}}

</main>